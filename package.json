  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "tsc": "node_modules\\.bin\\tsc -p build\\tsconfig.json",
    "clean": "rm -rf dist\\ng-notes functions\\dist",
    "clean:node": "rm -rf node_modules functions\\node_modules",
    "build-prod": "ng build --prod --output-hashing none",
    "build": "npm run clean && npm run build-prod && npm run tsc && node dist/build/build.js",
    "serve": "npm run build && firebase serve",
    "deploy": "npm run build && firebase deploy",
    "publish": "ng build ng-animate-value --prod && cd dist\\ng-animate-value && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
