{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "paths": {
      "ng-idle-click": [
        "dist/ng-idle-click"
      ],
      "ng-idle-click/*": [
        "dist/ng-idle-click/*"
      ],
      "ng-input-file": [
        "dist/ng-input-file"
      ],
      "ng-input-file/*": [
        "dist/ng-input-file/*"
      ],
      "ng-scrolltop": [
        "dist/ng-scrolltop"
      ],
      "ng-scrolltop/*": [
        "dist/ng-scrolltop/*"
      ],
      "ng-lazy-load": [
        "dist/ng-lazy-load"
      ],
      "ng-lazy-load/*": [
        "dist/ng-lazy-load/*"
      ],
      "ng-animate-value": [
        "dist/ng-animate-value"
      ],
      "ng-animate-value/*": [
        "dist/ng-animate-value/*"
      ]
    }
  }
}